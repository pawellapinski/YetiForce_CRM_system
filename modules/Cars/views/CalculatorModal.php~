<?php
// modules/Cars/views/CalculatorModal.php

class Cars_CalculatorModal_View extends Vtiger_BasicModal_View
{
    public function process(App\Request $request)
    {
        $viewer = $this->getViewer($request);
        $this->preProcess($request);
        $viewer->view('CalculatorModal.tpl', $request->getModule());
        $this->postProcess($request);
    }
}